    "build-storybook": "storybook build",
    "build": "pnpm build-ts && pnpm copyjs && node postbuild-inject.js",
    "build-ts": "pnpm tsc -p tsconfig.build.json",
    "copyjs": "rsync -avm --include='*/' --include='*.js' --exclude='*' lib/ dist/lib/",
    "publish-coneto": "pnpm build && pnpm publish",
    "generate-barrel": "barrelsby --excludePattern \\.stories\\.",
    "type-check": "tsc --noEmit",
    "lint": "eslint '**/*.{ts,tsx}'",
    "format": "prettier --write .",
    "build-chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "test-open": "cypress open",
    "test-run": "cypress run"
  },
